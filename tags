Delete	trie.cpp	/^void Delete( TrieNode * location)$/;"	f
Insert	trie.cpp	/^void Insert(TrieNode *root,const char *word)$/;"	f
Search	trie.cpp	/^bool Search( TrieNode *root ,const char *word)$/;"	f
TrieNode	trie.cpp	/^struct  TrieNode$/;"	s	file:
branchNum	trie.cpp	/^const int branchNum=26;	\/\/26个英文字母$/;"	v
isStr	trie.cpp	/^	bool isStr;	\/\/标示是否是一个完整的字符串$/;"	m	struct:TrieNode	file:
main	trie.cpp	/^int main()$/;"	f
next	trie.cpp	/^	TrieNode *next[branchNum];	\/\/每个字符的Trie树分支可能存在26个分支$/;"	m	struct:TrieNode	file:
